  },
  "devDependencies": {
    "babel": "^5.8.5",
    "babel-eslint": "^3.1.26",
    "eslint": "^0.24.1",
    "eslint-config-standard": "^3.4.1"
  },
  "files": [
    "LICENSE",
    "README.md",
    "src",
    "bin"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/itszero/es6-import-sorter"
