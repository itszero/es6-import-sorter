{
  "name": "es6-import-sorter",
  "version": "0.0.1",
  "description": "Sort imports for ES6 scripts",
  "bin": {
    "es6-import-sorter": "./bin/cli.js"
  },
  "main": "src/import-sorter.js",
  "scripts": {
    "start": "babel-node src/index.js",
    "build": "mkdir bin && babel -f bin/import_sorter.js src/index.js"
  },
  "keywords": [
    "import",
    "sort",
    "es6"
  ],
  "author": "Zero Cho <itszero@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "acorn": "^2.1.0",
    "babel-runtime": "^5.8.5",
    "escodegen": "^1.6.1"
  },
  "devDependencies": {
    "babel": "^5.8.5",
    "babel-eslint": "^3.1.26",
    "eslint": "^0.24.1",
    "eslint-config-standard": "^3.4.1"
  },
  "files": [
    "LICENSE",
    "README.md",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/itszero/es6-import-sorter"
  }
}
